union Option {
    Some(string),
    None,
}

union Result {
    Ok(string),
    Err(string),
}

fn add(a: i32, b: i32): i32 {
    a + b
}

fn sub(a: i32, b: i32): i32 {
    a - b
}

fn gt5od(a: i32): i32 {
    if a > 5 {
        return a;
    }

    a * 2
}