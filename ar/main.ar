struct Request {
    id: String,
    method: String,
}

struct Params<T> {
    params: T
}

struct InitializeRequest {
    Request { method: "initialize" },
    Params<InitializeRequestParams>,
}

struct InitializeRequestParams {
    client_id: Int
}

let init_req = InitializeRequest {
    id: "12345678",
    params: InitializeRequestParams {
        client_id: 0
    }
}
